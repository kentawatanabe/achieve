<div class = "container">
  <h2>ユーザー</h2>
  <% @users.each do |user| %>
    <div class= "container-child col-md-4 col-sm-4 col-xs-6">
      <h1><%=""%></h1>
      <%= link_to user_path(user) do %>
        <%= user.name %>
      <% end %>
      <div class = "profile_image">
        <%= profile_img(user) %>
      </div>
      <%= render 'follow_form', user: user %>
    </div>
  <% end %>
</div>
